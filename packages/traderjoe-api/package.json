{
  "name": "@hovoh/traderjoe-api",
  "version": "0.1.3",
  "license": "MIT",
  "author": "HOVOH ",
  "main": "dist/index.js",
  "module": "dist/traderjoe-api.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "generated"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "generate:typings": "typechain --target node_modules/@hovoh/typechain-ethers-multicall --out-dir ./generated './node_modules/@traderjoe-xyz/core/abi/**/*.json'",
    "build": "yarn generate:typings && dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/traderjoe-api.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/traderjoe-api.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@hovoh/ethcall": "^9.9.8",
    "@hovoh/evmcontractsregistry": "0.3.3",
    "@hovoh/typechain-ethers-multicall": "^10.1.3",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@traderjoe-xyz/core": "^2.4.6",
    "@tsconfig/recommended": "^1.0.1",
    "dts-cli": "^1.6.0",
    "ethers": "^5.7.1",
    "husky": "^8.0.1",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.0",
    "typechain": "^8.1.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@hovoh/ethcall": "^9.9.8",
    "@hovoh/evmcontractsregistry": "0.3.3",
    "ethers": "^5.7.1"
  }
}
