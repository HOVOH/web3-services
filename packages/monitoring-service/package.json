{
  "name": "monitoring-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "docker:up": "docker compose -f docker-compose.yml -f production-compose.yml up -d",
    "docker:build:up": "docker compose -f docker-compose.yml -f build-compose.yml up -d",
    "docker:dev": "docker compose up -d",
    "cmd": "ts-node ./src/cli.ts"
  },
  "dependencies": {
    "@hovoh/chainlink-api": "0.1.5",
    "@hovoh/ethcall": "^9.9.9",
    "@hovoh/evmcontractsregistry": "0.3.5",
    "@hovoh/olympuspro-api": "0.1.2",
    "@hovoh/openzeppelin-api": "0.1.4",
    "@hovoh/traderjoe-api": "0.1.3",
    "@hovoh/uniswapv2-api": "0.1.5",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@uniswap/sdk-core": "^3.1.0",
    "@uniswap/smart-order-router": "^2.10.0",
    "@uniswap/v3-sdk": "^3.9.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.7.1",
    "nest-commander": "^3.1.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "typeorm": "^0.3.7",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^18.7.18",
    "@types/pg": "^8",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^26.6.3",
    "prettier": "^2.7.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
